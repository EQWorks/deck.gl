{
  "name": "kepler-outdated-deck.gl-jupyter-widget",
  "description": "Jupyter widget for rendering deck.gl in a Jupyter notebook",
  "license": "MIT",
  "version": "7.1.1",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets",
    "geospatial"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/deck.gl.git"
  },
  "main": "dist/pydeck_embeddable/index.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "webpack-bundle": "webpack",
    "notebook-bundle": "npm install --save-dev && npm run webpack-bundle"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10",
    "@types/mapbox-gl": "^0.51.4",
    "mapbox-gl": "^0.53.1"
  },
  "devDependencies": {
    "kepler-outdated-deck.gl-aggregation-layers": "file:../aggregation-layers",
    "kepler-outdated-deck.gl-core": "file:../core",
    "kepler-outdated-deck.gl-json": "file:../json",
    "kepler-outdated-deck.gl-layers": "file:../layers"
  },
  "peerDependencies": {
    "kepler-outdated-deck.gl-aggregation-layers": "https://github.com/EQWorks/deck.gl/raw/build/outdated/modules/aggregation-layers/dist.tgz",
    "kepler-outdated-deck.gl-core": "https://github.com/EQWorks/deck.gl/raw/build/outdated/modules/core/dist.tgz",
    "kepler-outdated-deck.gl-json": "https://github.com/EQWorks/deck.gl/raw/build/outdated/modules/json/dist.tgz",
    "kepler-outdated-deck.gl-layers": "https://github.com/EQWorks/deck.gl/raw/build/outdated/modules/layers/dist.tgz"
  }
}
